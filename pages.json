{
    "pages": [{
        "path": "pages/tabbar/index/index",
        "style": {
            // #ifdef H5
            "titleNView": false,
            // #endif
            "navigationBarTitleText": "威四方",
            "app-plus": {
                "bounce": "vertical",
                // #ifdef APP-PLUS
                "titleNView": {
                    "buttons": [{
                        "id": "home",
                        "uni_code": "home_menu",
                        "float": "right",
                        "fontSize": "24px",
                        "fontSrc": "/static/fonts/iconfont.ttf", // 也可以使用字体icon uni-app
                        "text": "\ue704",
                        "width": "auto"
                    }]
                }
                // #endif
            }
        }
    }, {
        "path": "pages/tabbar/work/work",
        "style": {
            // #ifdef H5
            "titleNView": false,
            // #endif
            "navigationBarTitleText": "应用",
            "app-plus": {
                "bounce": "vertical",
                "titleNView": {
                    "buttons": [{
                        "id": "work",
                        "uni_code": "work_menu",
                        "float": "right",
                        "fontSize": "24px",
                        "fontSrc": "/static/fonts/iconfont.ttf", // 也可以使用字体icon uni-app
                        "text": "\ue704",
                        "width": "auto"
                    }]
                }
            }
        }
    }, {
        "path": "pages/tabbar/my/my",
        "style": {
            // #ifdef MP-ALIPAY
            "navigationBarTitleText": "",
            // #endif
            "navigationBarBackgroundColor": "#fff",
            "navigationBarTextStyle": "black",
            "enablePullDownRefresh": true,
            "onReachBottomDistance": 50,
            "navigationStyle": "custom",
            "app-plus": {
                "titleNView": false,
                "bounce": "vertical"
            },
            "mp-alipay": {
                "allowsBounceVertical": "YES",
                "transparentTitle": "always",
                "titlePenetrate": "YES"
            }
        }
    }, {
        "path": "pages/common/login/login",
        "style": {
            "backgroundColorTop": "#5677FC",
            "backgroundColorBottom": "#ffffff",
            "navigationBarTitleText": "威四方",
            "enablePullDownRefresh": false,
            "navigationStyle": "custom",
            "app-plus": {
                "bounce": "vertical"
            }
        }
    }, {
        "path": "pages/common/login/register",
        "style": {
            "navigationBarTitleText": "注册",
            "enablePullDownRefresh": false,
            "titleNView": false,
            "navigationStyle": "custom"
        }
    }, {
        "path": "pages/common/login/forget",
        "style": {
            "navigationBarTitleText": "忘记密码",
            "enablePullDownRefresh": false,
            "titleNView": false,
            "navigationStyle": "custom"
        }
    }, {
        "path": "pages/common/ba/baScanView",
        "style": {
            "navigationBarTitleText": "扫一扫",
            "enablePullDownRefresh": false
        }
    }, {
        "path": "pages/common/webview/webview",
        "style": {
            "navigationBarTitleText": "",
            "enablePullDownRefresh": false,
            "app-plus": {
                "titleNView": {
                    "buttons": [{
                        "id": "2",
                        "uni_code": "webview_menu",
                        "float": "right",
                        "fontSize": "24px",
                        "fontSrc": "/static/fonts/iconfont.ttf", // 也可以使用字体icon uni-app
                        "text": "\ue704",
                        "width": "auto"
                    }]
                }
            }
        }
    }, {
        "path": "pages/common/msg/msg",
        "style": {
            "navigationBarTitleText": "识别结果",
            "enablePullDownRefresh": false
        }
    }, {
        "path": "pages/doc/network/network",
        "style": {
            // #ifdef H5
            "titleNView": false,
            // #endif
            "navigationBarTitleText": "未能连接到互联网",
            "app-plus": {
                "bounce": "vertical"
            }
        }
    }, {
        "path": "pages/doc/protocol/protocol",
        "style": {
            // #ifdef H5
            "titleNView": false,
            // #endif
            "navigationBarTitleText": "用户协议",
            "app-plus": {
                "bounce": "vertical"
            }
        }
    }, {
        "path": "pages/common/doc/log",
        "style": {
            "navigationBarTitleText": "更新日志",
            "app-plus": {
                "bounce": "vertical"
            }
        }
    }, {
        "path": "pages/common/contactUs/contactUs",
        "style": {
            "navigationBarTitleText": "联系我们",
            "app-plus": {
                "bounce": "vertical"
            }
        }
    }, {
        "path": "components/app-upgrade/views/app-upgrade", //第2步新建的app-uodate.vue文件
        "style": {
            "navigationBarTitleText": "应用更新",
            "navigationStyle": "custom",
            "app-plus": {
                "background": "transparent",
                "webviewBGTransparent": true, //webview的背景是否透明//配合background属性防止跳转闪白
                "popGesture": "none" //禁止侧滑关闭（ios）
            }
        }
    }, {
        "path": "pages/common/doc/protocol",
        "style": {
            "navigationBarTitleText": "用户协议",
            "enablePullDownRefresh": false
        }
    }, {
        "path": "pages/common/doc/about",
        "style": {
            "navigationBarTitleText": "关于我们",
            "enablePullDownRefresh": false
        }
    }, {
        "path": "pages/common/setting/setting",
        "style": {
            "navigationBarTitleText": "应用设置",
            "enablePullDownRefresh": false
        }
    }, {
        "path": "pages/common/test/test",
        "style": {
            "navigationBarTitleText": "功能测试",
            "enablePullDownRefresh": false
        }
    }],
    "preloadRule": {
        "pages/tabbar/index/index": {
            "network": "all",
            "packages": ["pagesCommon"]
        },
        "pages/tabbar/work/work": {
            "network": "all",
            "packages": ["pagesWork", "pagesShop"]
        }
    },
    "subPackages": [{
        "root": "pagesCommon",
        "pages": []
    }, {
        "root": "pagesWork",
        "pages": []
    }, {
        "root": "pagesShop",
        "pages": [{
            "path": "tabbar/menu",
            "style": {
                // #ifndef MP
                "titleNView": false,
                // #endif
                "navigationStyle": "custom",
                "navigationBarTitleText": "",
                "navigationBarTextStyle": "white",
                "app-plus": {
                    "bounce": "vertical"
                },
                "mp-alipay": {
                    "transparentTitle": "always",
                    "titlePenetrate": "YES"
                }
            }
        }, {
            "path": "orderDetail/orderDetail",
            "style": {
                "navigationBarTitleText": "订单详情",
                "app-plus": {
                    "bounce": "vertical"
                }
            }
        }, {
            "path": "submitOrder/submitOrder",
            "style": {
                "navigationBarTitleText": "账单结算",
                "app-plus": {
                    "bounce": "vertical"
                }
            }
        }, {
            "path": "address/address",
            "style": {
                "navigationBarTitleText": "收货地址",
                "app-plus": {
                    "bounce": "vertical"
                }
            }
        }, {
            "path": "editAddress/editAddress",
            "style": {
                "navigationBarTitleText": "编辑地址",
                "app-plus": {
                    "bounce": "vertical"
                }
            }
        }, {
            "path": "verifyLogin/verifyLogin",
            "style": {
                "navigationBarTitleText": "登录",
                "app-plus": {
                    "bounce": "vertical"
                }
            }
        }]
    }],
    "easycom": {
        "autoscan": true,
        "custom": {
            "tui-(.*)": "@/components/thorui/tui-$1/tui-$1.vue",
            "wsf-(.*)": "@/components/weisifang/wsf-$1/wsf-$1.vue"
        }
    },
    "globalStyle": {
        "backgroundTextStyle": "dark",
        "navigationBarBackgroundColor": "#5677FC",
        "navigationBarTitleText": "威四方",
        "navigationBarTextStyle": "white",
        "backgroundColor": "#fafafa",
        "backgroundColorTop": "#fafafa",
        "backgroundColorBottom": "#fafafa"
    },
    "tabBar": {
        "color": "#666666",
        "selectedColor": "#5677FC",
        "backgroundColor": "#FFFFFF",
        "list": [{
                "pagePath": "pages/tabbar/index/index",
                "text": "首页",
                "iconPath": "static/images/tabbar/home_gray.png",
                "selectedIconPath": "static/images/tabbar/home_active.png"
            },
            {
                "pagePath": "pages/tabbar/work/work",
                "text": "应用",
                "iconPath": "static/images/tabbar/work_gray.png",
                "selectedIconPath": "static/images/tabbar/work_active.png"
            },
            {
                "pagePath": "pages/tabbar/my/my",
                "text": "我的",
                "iconPath": "static/images/tabbar/user_gray.png",
                "selectedIconPath": "static/images/tabbar/user_active.png"
            }
        ]
    }
}